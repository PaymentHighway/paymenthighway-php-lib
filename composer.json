{
  "name": "solinor/paymenthighwayio",
  "description": "Paymenthighway SDK",
  "type": "library",
  "authors" : [
    {
      "name" : "Jonni Larjomaa",
      "email" : "jonni.larjomaa@solinor.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ramsey/uuid" : "^2.8",
    "nategood/httpful": "*",
    "respect/validation": "^1.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "4.8.*"
  },
  "autoload" : {
    "psr-4": {
      "Solinor\\PaymentHighway\\": "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Solinor\\PaymentHighway\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability" : "stable"
}
